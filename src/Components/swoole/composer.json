{
    "name": "imiphp/imi-swoole",
    "type": "library",
    "license": "MulanPSL-1.0",
    "description": "imi swoole component",
    "require": {
        "ext-swoole": ">=4.7",
        "yurunsoft/swoole-co-pool": "^1.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8",
        "swoole/ide-helper": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "Imi\\Swoole\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imi\\Swoole\\Test\\": "tests/unit/"
        }
    },
    "bin": [
        "bin/imi-swoole",
        "bin/swoole-phpunit"
    ]
}